{
	"name": "quick-dry-login",
	"version": "1.0.4",
	"description": "Manually log into your WP development or staging website.",
	"author": "badasswp",
	"license": "GPL-2.0-or-later",
	"homepage": "https://github.com/badasswp/quick-dry-login#readme",
	"scripts": {
		"start": "yarn boot && yarn wp-env start",
		"stop": "yarn wp-env stop",
		"boot": "composer install && yarn install",
		"lint:php": "composer run lint",
		"lint:php:fix": "composer run lint:fix",
		"ci": "yarn lint:php",
		"wp-env": "wp-env",
		"bash": "wp-env run cli bash"
	},
	"devDependencies": {
		"@wordpress/env": "^10.20.0"
	},
	"keywords": [
		"quick",
		"dry",
		"login"
	]
}
